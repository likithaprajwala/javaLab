package Com.r9;

public class today {
    String fname = "likitha";
    String lname = "praj";
    int age = 18; 
    today(){
    System.out.println("default constructor");
    }
  public static void main(String[] args) {
       today myObj = new today();
       System.out.println("Name: " + myObj.fname + " " + myObj.lname);
       System.out.println("Age: " + myObj.age);
  }
}
